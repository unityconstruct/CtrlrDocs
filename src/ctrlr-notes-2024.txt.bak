--- lcdLabel utilities

--- fetch an label component by its name
--- @param stringName string - value to pass to panel:getComponent()
--- @return a display component
function getLcdLabelByName(stringName)
    return panel:getComponent(stringName)
end


function lcdMultiUpdateText(comp, valueString)
    local lcd = comp
    if tostring(type(comp)) == "string" then
        lcd = getLcdLabelByName(comp)
    end
    lcd.setPropertyString(lcd,"uiLabelText",valueString)
end




